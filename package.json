    "pages:deploy": "npm run build && wrangler pages deploy ./public",
    "lint": "eslint app",
    "format": "prettier app --write"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.19.3",
    "@remix-run/cloudflare-pages": "^1.19.3",
